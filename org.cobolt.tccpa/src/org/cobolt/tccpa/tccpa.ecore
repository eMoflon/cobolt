<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="tccpa" nsURI="http://www.bank.com" nsPrefix="org.cobolt.tccpa">
  <eClassifiers xsi:type="ecore:EClass" name="Topology">
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodes" upperBound="-1"
        eType="#//Node" containment="true" eOpposite="#//Node/topology"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="links" upperBound="-1"
        eType="#//Link" containment="true" eOpposite="#//Link/topology"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="triangles" upperBound="-1"
        eType="#//PhiTriangle" containment="true" eOpposite="#//PhiTriangle/topology"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Node" eSuperTypes="#//TopologyElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="topology" eType="#//Topology"
        eOpposite="#//Topology/nodes"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="outgoingLinks" upperBound="-1"
        eType="#//Link" eOpposite="#//Link/source"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="incomingLinks" upperBound="-1"
        eType="#//Link" eOpposite="#//Link/target"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Link" eSuperTypes="#//TopologyElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="weight" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="state" eType="#//LinkState"
        defaultValueLiteral="UNMARKED"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="topology" eType="#//Topology"
        eOpposite="#//Topology/links"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
        eType="#//Node" eOpposite="#//Node/outgoingLinks"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//Node" eOpposite="#//Node/incomingLinks"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="baseLinkInTriangle" upperBound="-1"
        eType="#//PhiTriangle" eOpposite="#//PhiTriangle/base"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="leftLinkInTriangles" upperBound="-1"
        eType="#//PhiTriangle" eOpposite="#//PhiTriangle/left"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="rightLinkInTriangles" upperBound="-1"
        eType="#//PhiTriangle" eOpposite="#//PhiTriangle/right"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="LinkState">
    <eLiterals name="ACTIVE"/>
    <eLiterals name="INACTIVE" value="1" literal="INACTIVE"/>
    <eLiterals name="UNMARKED" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TopologyElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PhiTriangle">
    <eStructuralFeatures xsi:type="ecore:EReference" name="base" lowerBound="1" eType="#//Link"
        eOpposite="#//Link/baseLinkInTriangle"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" lowerBound="1" eType="#//Link"
        eOpposite="#//Link/leftLinkInTriangles"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" lowerBound="1" eType="#//Link"
        eOpposite="#//Link/rightLinkInTriangles"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="topology" eType="#//Topology"
        eOpposite="#//Topology/triangles"/>
  </eClassifiers>
</ecore:EPackage>
